<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>

<TableView fx:id="MarkPointTable" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="PixelRTGHelper.MarkPointTableController">
    <contextMenu>
        <ContextMenu fx:id="MarkPointContextMenu">
            <items>
                <MenuItem text="Remove" fx:id="MarkPointContextMenuRemove"/>
            </items>
        </ContextMenu>
    </contextMenu>
    <columns>
        <TableColumn fx:id="MarkPointTableName" prefWidth="100.0" text="Name">
        </TableColumn>
        <TableColumn fx:id="MarkPointTablePositionX" prefWidth="40.0" text="X">
        </TableColumn>
        <TableColumn fx:id="MarkPointTablePositionY" prefWidth="40.0" text="Y">
        </TableColumn>
        <TableColumn fx:id="MarkPointTableColor" prefWidth="18.0" text="C">
        </TableColumn>
    </columns>
</TableView>
